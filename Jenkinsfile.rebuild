node {
  checkout scm
  withEnv([
    'USERNAME=test',
    'URL=localhost:7070',
    'JOB=test',
    'ID=remote',
  ]) 
  {  
   withCredentials(
      [
        string(credentialsId: 'key', variable: 'key')        
      ]
    ) {  
        stage('Deploy') {  
     
              sh "docker run -v /var/run/docker.sock:/var/run/docker.sock docker.io/jenkins/jenkins:lts sh -c 'docker exec -it gatsby /bin/bash'"
       }
     }
    }
}



